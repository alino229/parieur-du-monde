{% extends 'admin_base.html.twig' %}

{% block title %}Article index{% endblock %}

{% block body %}
    <h1>Vip</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vip.id }}</td>
            </tr>
            <tr>
                <th>Abonnement</th>
                <td>{{ vip.abonnement }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ vip.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ vip.date ? vip.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ExpireDate</th>
                <td>{{ vip.expireDate ? vip.expireDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('vip_index') }}">back to list</a>

    <a href="{{ path('vip_edit', {'id': vip.id}) }}">edit</a>

    {{ include('admin/vip/_delete_form.html.twig') }}
{% endblock %}
